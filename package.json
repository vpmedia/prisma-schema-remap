{
  "name": "@vpmedia/prisma-schema-remap",
  "version": "1.3.0",
  "description": "Give your introspected schema.prisma sane casing conventions",
  "main": "./dist/cli.js",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/vpmedia/prisma-schema-remap.git"
  },
  "bin": {
    "prisma-schema-remap": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "cli": "node ./bin/cli.js",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json,css}\"",
    "lint": "eslint",
    "test": "vitest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.1.0",
    "@types/pluralize": "0.0.33",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "^62.5.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@prisma/internals": "^7.3.0",
    "chalk": "^5.6.2",
    "change-case": "5.4.4",
    "commander": "14.0.2",
    "js-yaml": "4.1.1",
    "pluralize": "8.0.0"
  },
  "pnpm": {
    "auditConfig": {
      "ignoreGhsas": [
        "GHSA-73rr-hh4g-fpgx"
